  "css": "* {\r\n  padding: 0;\r\n  margin: 0;\r\n  box-sizing: border-box;\r\n}\r\n\r\n.main_logo {\r\n  display: flex;\r\n  justify-content: space-evenly;\r\n  align-items: center;\r\n  border-bottom: 1px solid gray;\r\n  margin: 0px 10px 0px 10px;\r\n}\r\n.logo_left img {\r\n  width: 300px;\r\n  height: 100px;\r\n}\r\n.right_logo h4 {\r\n  text-align: center;\r\n}\r\n.right_logo {\r\n  display: flex;\r\n}\r\n.verifed_icon {\r\n  font-size: 40px;\r\n  color: green;\r\n}\r\n.number {\r\n  padding: 14px 0px 0px 30px;\r\n  font-size: 25px;\r\n}\r\n/* .................. mian_info */\r\nh1,\r\nh2,\r\nh3,\r\nh4,\r\nh5,\r\nh6{\r\n    padding:0px;\r\n    margin:0px;\r\n}\r\n.mian_info {\r\n  display: flex;\r\n  width: 100%;\r\n  justify-content: center;\r\n  padding-top: 10px;\r\n}\r\n.info_2 {\r\n  margin-left: 100px;\r\n}\r\n.info_id,\r\n.info_age {\r\n  display: flex;\r\n  padding: 10px 0px 0px 0px;\r\n}\r\n\r\n.info_id_2,\r\n.info_age_2,\r\n.info_doctor_2 {\r\n  display: flex;\r\n  padding: 10px 0px 0px 0px;\r\n}\r\n/* ...... */\r\n.info_id h4 {\r\n  color: #9478a9;\r\n  margin-left: 90px;\r\n  border: 1px solid rgb(141, 225, 141);\r\n  border-radius: 50px;\r\n  text-align: left;\r\n  padding: 4px 50px 4px 50px;\r\n}\r\n.info_age h4 {\r\n  color: #9478a9;\r\n  margin-left: 70px;\r\n}\r\n.info_doctor h4 {\r\n  color: #9478a9;\r\n  padding-left: 64px;\r\n}\r\n/* ....... */\r\n.info_id_2 h4 {\r\n  color: #9478a9;\r\n  margin-left: 100px;\r\n  border: 1px solid rgb(141, 225, 141);\r\n  border-radius: 50px;\r\n\r\n  padding: 4px 50px 4px 50px;\r\n}\r\n.info_age_2 h4 {\r\n  color: #9478a9;\r\n  padding-left: 127px;\r\n}\r\n.info_doctor_2 h4 {\r\n  color: #9478a9;\r\n  padding-left: 125px;\r\n}\r\n\r\n.info_id h3,\r\n.info_age h3 {\r\n  color: #153b50;\r\n}\r\n\r\n/* ........... table */\r\n/* ...........table_section */\r\n.table_section {\r\n  display: flex;\r\n  justify-content: center;\r\n  padding-top: 10px;\r\n  margin: 10px;\r\n}\r\ntable {\r\n  border-collapse: collapse;\r\n}\r\n\r\ntable td {\r\n  border-bottom: 1px solid gray;\r\n  border-top: 1px solid gray;\r\n  padding: 20px 0px 20px 0px;\r\n}\r\ntable th {\r\n  padding: 10px 30px 10px 30px;\r\n  text-align: left;\r\n  color: gray;\r\n  font-size: 12px;\r\n}\r\ntable th:nth-child(2) {\r\n  width: 460px;\r\n}\r\n\r\ntable td {\r\n  padding: 10px 30px 10px 30px;\r\n  text-align: left;\r\n  /* color: rgb(90, 71, 109); */\r\n  color: #342042;\r\n}\r\n\r\n/* ............... sub totals */\r\n.finance_office {\r\n  display: flex;\r\n  flex-direction: column;\r\n  margin-left: 260px;\r\n  padding-top: 20px;\r\n}\r\n.finance_office h3{\r\n    padding-left: 50px;\r\n}\r\n.main_total {\r\n  float: right;\r\n  margin-right: 160px;\r\n}\r\n.total,\r\n.discount,\r\n.grand,\r\n.amount,\r\n.balance {\r\n  display: flex;\r\n  margin-right: 30px;\r\n  padding: 3px 0px 3px 0px;\r\n  color: rgb(88, 88, 88);\r\n}\r\n.total h4 {\r\n  padding-left: 80px;\r\n  text-align: left;\r\n}\r\n.discount h4 {\r\n  padding-left: 80px;\r\n  text-align: left;\r\n}\r\n.grand h4 {\r\n  padding-left: 50px;\r\n  text-align: left;\r\n}\r\n.amount h4 {\r\n  padding-left: 40px;\r\n  text-align: left;\r\n}\r\n.balance h4 {\r\n  padding-left: 40px;\r\n  text-align: center;\r\n  border: 1px solid rgb(141, 225, 141);\r\n  border-radius: 50px;\r\n  padding: 3px 50px 3px 50px;\r\n}\r\n.thanks {\r\n  /*padding-top: 170px;*/\r\n  /*color: rgb(88, 88, 88);*/\r\n  /*display: flex;*/\r\n  /*justify-content:center;*/\r\n\r\n}\r\n\r\n/* ..................... adress */\r\n.thanks{\r\n    display:flex;\r\n    justify-content:center;\r\n     border-bottom: 2px dotted #1c146b;\r\n}\r\n.main_address{\r\n    position:fixed;\r\n    bottom:0;\r\n  \r\n}\r\n.address {\r\n    display: flex;\r\n    justify-content: center;\r\n    padding-top: 20px;\r\n    color: #342042;\r\n   \r\n  }\r\n\r\n  .address h4 {\r\n    font-size: 14px;\r\n  }\r\n  .address h4 span {\r\n    padding: 0px 5px 0px 5px;\r\n  }\r\n  \r\n@media print {\r\n  .main_total {\r\n    float: right;\r\n    margin-right: 80px;\r\n  }\r\n  .finance_office {\r\n    display: flex;\r\n    flex-direction: column;\r\n    margin-left: 100px;\r\n  \r\n    padding-top: 20px;\r\n  }\r\n  .finance_office h3{\r\n        text-align: left;\r\n  }\r\n.info_id_2 h3:nth-child(1){\r\n  width: 100px;\r\n}\r\n.info_id_2 h4 {\r\n  color: #9478a9;\r\n  margin-left: 50px;\r\n  border: 1px solid rgb(141, 225, 141);\r\n  border-radius: 50px;\r\n\r\n  padding: 4px 50px 4px 50px;\r\n}\r\n\r\n}\r\n",
