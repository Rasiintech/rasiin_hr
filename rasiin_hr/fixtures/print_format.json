  "html": "  \r\n  {{ letter_head }}\r\n    <header class=\"header mx\">\r\n   \r\n      <div class=\"header_right\">\r\n       \r\n        <span> <b>Ka: </b>Baraka hospital</span>\r\n          <span> <b>Ku:</b> {{doc.patient_name}}</span>\r\n          <span><b>Ujeedo:</b> Qaansheegad</span>\r\n           <span><b> Date:</b> {{ doc.date }}</span>\r\n      </div>\r\n      \r\n      <div class=\"header_right\">\r\n        <span> <b>Address: </b>Warshada Caanaha, Hodan</span>\r\n        <span><b>Tell:</b> 0613922225/0613922226 </span>\r\n        <span><b>Email: </b>info@barakahospital.com</span>\r\n        <span> <b>Website:</b> www.barakahospital.com</span>\r\n      </div>\r\n    </header>\r\n\r\n    <section class=\"dahir_table_sec mx\">\r\n      <table style=\"width: 100%\">\r\n        <thead>\r\n          <tr>\r\n            <th style=\"text-align: start; padding-left: 5px\">No</th>\r\n            <th style=\"text-align: start; padding-left: 5px\">Name</th>\r\n            <th style=\"text-align: start; padding-left: 5px\">Tell Number</th>\r\n            <th style=\"text-align: start; padding-left: 5px\">Pharmacy</th>\r\n            <th style=\"text-align: end;\" >Other Service</th>\r\n          </tr>\r\n        </thead>\r\n        <tbody>\r\n          <tr>\r\n            <th style=\"text-align: start\">1</th>\r\n            <th\r\n              style=\"\r\n                text-align: start;\r\n                text-transform: uppercase;\r\n                font-size: 14px;\r\n                padding-left: 5px;\r\n              \"\r\n            >\r\n             {{doc.patient_name}}\r\n            </th>\r\n            <th style=\"text-align: start; padding-left: 5px\">{{doc.mobile}}</th>\r\n            <th style=\"text-align: start; padding-left: 5px\">${{doc.pharmacy}}</th></th>\r\n            <th style=\"text-align: end; \">${{doc.others}}</th>\r\n          </tr>\r\n          \r\n                <tr>\r\n           \r\n              <th style=\"text-align: start; padding-left: 5px;   border-left: 1px solid white;    border-bottom: 1px solid white;  \" colspan= \"3\"></th>\r\n          \r\n            <th style=\"text-align: start; padding-left: 5px ;  border-right: 1px solid white; \">Total</th>\r\n            <th style=\"text-align: start; padding-left: 5px; border-left: 1px solid white; text-align: right;\">${{doc.balance}}</th>\r\n         \r\n          </tr>\r\n          \r\n          <tr>\r\n           \r\n              <th style=\"text-align: start; padding-left: 5px;   border-left: 1px solid white;    border-bottom: 1px solid white;  \" colspan= \"3\"></th>\r\n          \r\n            <th style=\"text-align: start; padding-left: 5px ;   border-right: 1px solid white;\"  >Discount</th>\r\n            <th style=\"text-align: start; padding-left: 5px ; border-left: 1px solid white; text-align: right;\">${{ doc.discount}}</th>\r\n         \r\n          </tr>\r\n          \r\n          <tr>\r\n           \r\n              <th style=\"text-align: start; padding-left: 5px;   border-left: 1px solid white;    border-bottom: 1px solid white;  \" colspan= \"3\"></th>\r\n          \r\n            <th style=\"text-align: start; padding-left: 5px ;   border-right: 1px solid white;\" >Net Amount</th>\r\n            <th style=\"text-align: start; padding-left: 5px; border-left: 1px solid white; text-align: right;\"> ${{ doc.net_amount}}</th>\r\n         \r\n          </tr>\r\n          \r\n          \r\n          \r\n        </tbody>\r\n      </table>\r\n    </section>\r\n\r\n    <!--<section class=\"total_sec mx\">-->\r\n    <!--  <div class=\"column\">-->\r\n    <!--    <span>Salaam bank account</span>-->\r\n    <!--    <span>Baraka Hospital</span>-->\r\n    <!--    <span>0033333</span>-->\r\n    <!--  </div>-->\r\n\r\n    <!--  <div class=\"column\">-->\r\n    <!--    <div class=\"row\">-->\r\n    <!--      <span style=\"font-size: 16px; text-transform: uppercase\"-->\r\n    <!--        >Grand total</span-->\r\n    <!--      >-->\r\n    <!--      <span>$444</span>-->\r\n    <!--    </div>-->\r\n    <!--    <div class=\"row\">-->\r\n    <!--      <span style=\"font-size: 16px;  text-transform: uppercase\"-->\r\n    <!--        >Discount</span-->\r\n    <!--      >-->\r\n    <!--      <span style=\"text-decoration: underline; font-weight: bold;\">$63</span>-->\r\n    <!--    </div>-->\r\n    <!--    <div class=\"row\">-->\r\n    <!--      <span style=\"font-size: 16px; text-transform: uppercase\"-->\r\n    <!--        >Net Amount</span-->\r\n    <!--      >-->\r\n    <!--      <span style=\"font-weight: bold\">$999</span>-->\r\n    <!--    </div>-->\r\n    <!--  </div>-->\r\n    <!--</section>-->\r\n\r\n    <footer class=\"footer mx\">\r\n      <span style=\"font-size: 16px\"\r\n        ><B>Cashier: </B>{{ frappe.db.get_value(\"User\" , frappe.session.user , \"full_name\")}}</span\r\n      >\r\n   \r\n      <span style=\"font-size: 16px\"><b>Email:</b> {{ frappe.session.user }}</span>\r\n      <span style=\"font-size: 16px\"><b>Tell no: </b>0613922225/0613922226</span>\r\n      <span  style=\"font-size: 16px\"><b>Signature:</b> _________________</span>\r\n      \r\n      <span  align =\"center\" style=\"margin-top : 300px\">\u00a9Rasiin Technology</span>\r\n    </footer>\r\n  \r\n",
