  "html": "<!DOCTYPE html>\r\n<html>\r\n<head>\r\n  <meta charset=\"UTF-8\" />\r\n  <title>Eye Glass Prescription</title>\r\n</head>\r\n<body>\r\n  <div class=\"container\">\r\n    <!-- Top Form Fields -->\r\n    <div class=\"form-section\">\r\n      <label>Patient ID:</label>\r\n      <span>{{ doc.patient or \"_______________\" }}</span>\r\n\r\n      <label style=\"margin-left:20px;\">Date:</label>\r\n      <span>{{ doc.encounter_date or \"___/___/____\" }}</span>\r\n    </div>\r\n\r\n    <div class=\"form-section\">\r\n      <label>Patient Name:</label>\r\n      <span>{{ doc.patient_name or \"__________________________\" }}</span>\r\n    </div>\r\n\r\n    <!-- Eye Prescription Table -->\r\n    <table>\r\n      <thead>\r\n        <tr>\r\n          <th></th>\r\n          <th colspan=\"3\">Right Eye (OD)</th>\r\n          <th colspan=\"3\">Left Eye (OS)</th>\r\n        </tr>\r\n        <tr>\r\n          <th></th>\r\n          <th>SPH</th>\r\n          <th>CYL</th>\r\n          <th>AXIS</th>\r\n          <th>SPH</th>\r\n          <th>CYL</th>\r\n          <th>AXIS</th>\r\n        </tr>\r\n      </thead>\r\n      <tbody>\r\n\r\n      {% for row in doc.glass_prescription %}\r\n     \r\n  {% if row.rows_fields == \"Constant\" %}\r\n  \r\n    <tr>\r\n      <td>Constant</td>\r\n      <td>{{ row.right_sph or \"\" }}</td>\r\n      <td>{{ row.right_cyl or \"\" }}</td>\r\n      <td>{{ row.right_axis or \"\" }}</td>\r\n      <td>{{ row.left_sph or \"\" }}</td>\r\n      <td>{{ row.left_cyl or \"\" }}</td>\r\n      <td>{{ row.left_axis or \"\" }}</td>\r\n    </tr>\r\n  {% endif %}\r\n{% endfor %}\r\n\r\n\r\n        <!-- DISTANCE ROW(s) -->\r\n        {% for row in doc.glass_prescription %}\r\n          {% if row.rows_fields == \"Distance\" %}\r\n            <tr>\r\n              <td>Distance</td>\r\n              <td>{{ row.right_sph or \"\" }}</td>\r\n              <td>{{ row.right_cyl or \"\" }}</td>\r\n              <td>{{ row.right_axis or \"\" }}</td>\r\n              <td>{{ row.left_sph or \"\" }}</td>\r\n              <td>{{ row.left_cyl or \"\" }}</td>\r\n              <td>{{ row.left_axis or \"\" }}</td>\r\n            </tr>\r\n          {% endif %}\r\n        {% endfor %}\r\n\r\n        <!-- NEAR ROW(s) -->\r\n        {% for row in doc.glass_prescription %}\r\n          {% if row.rows_fields == \"Near\" %}\r\n            <tr>\r\n              <td>Near</td>\r\n              <td>{{ row.right_sph or \"\" }}</td>\r\n              <td>{{ row.right_cyl or \"\" }}</td>\r\n              <td>{{ row.right_axis or \"\" }}</td>\r\n              <td>{{ row.left_sph or \"\" }}</td>\r\n              <td>{{ row.left_cyl or \"\" }}</td>\r\n              <td>{{ row.left_axis or \"\" }}</td>\r\n            </tr>\r\n          {% endif %}\r\n        {% endfor %}\r\n\r\n      </tbody>\r\n    </table>\r\n\r\n    <!-- Signature Section -->\r\n    <div class=\"signature\">\r\n      <label>Sign:</label>\r\n      <span>________________________</span>\r\n    </div>\r\n  </div>\r\n</body>\r\n</html>\r\n",
