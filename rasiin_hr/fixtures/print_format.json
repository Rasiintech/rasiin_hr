  "css": "@import url(\"https://fonts.googleapis.com/css2?family=Playfair+Display:ital@1&family=Poppins:wght@300;400;500&display=swap\");\r\n\r\n* {\r\n  padding: 0;\r\n  margin: 0;\r\n  box-sizing: border-box;\r\n}\r\n\r\nbody {\r\n  box-sizing: inherit;\r\n}\r\n\r\n/* .main{\r\n  background-color: rgb(240, 212, 212);\r\n} */\r\n\r\n/* ..................section 1 */\r\n.one {\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n.name {\r\n  font-weight: 200;\r\n}\r\n.name {\r\n  border-left: none;\r\n  border-right: none;\r\n  border-top: none;\r\n  border-bottom: 1px solid black;\r\n  padding: 3px 0px 4px 10px;\r\n}\r\n\r\n.h4_name {\r\n  color: black;\r\n  font-weight: bold;\r\n  font-size: 20px;\r\n  margin-top: 20px;\r\n  width: 100%;\r\n}\r\n\r\n.birth_p_one {\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: center;\r\n  align-items: center;\r\n  gap: 30px;\r\n}\r\n\r\n.header {\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: center;\r\n  flex-direction: column;\r\n  width: 100%;\r\n  margin-top: 50px;\r\n}\r\n.header:nth-child(1) {\r\n  text-align: center;\r\n}\r\n.header h2 {\r\n  font-size: 16px;\r\n  font-family: \"Playfair Display\", serif;\r\n  color: rgb(46, 46, 155);\r\n}\r\n\r\n.birth_info {\r\n  display: flex;\r\n  margin-left: 30px;\r\n}\r\n\r\n.info_left {\r\n  display: flex;\r\n  flex-direction: column;\r\n  gap: 20px;\r\n}\r\n\r\n.info_right {\r\n  display: flex;\r\n  gap: 20px;\r\n  margin-left: 30px;\r\n}\r\n\r\n.round {\r\n  height: 210px;\r\n  width: 120px;\r\n  border: 1px solid black;\r\n  border-radius: 40%;\r\n  margin-left: 20px;\r\n}\r\n\r\n/* .........................section 2 */\r\n.birth_p_two {\r\n  display: flex;\r\n  flex-direction: column;\r\n  gap: 40px;\r\n  justify-content: center;\r\n  align-items: center;\r\n  margin-top: 20px;\r\n}\r\n\r\n.birth_p_two_info {\r\n  display: flex;\r\n  flex-direction: column;\r\n  gap: 20px;\r\n  margin-left: 50px;\r\n}\r\n\r\n.palce_district {\r\n  display: flex;\r\n  align-items: center;\r\n  gap: 30px;\r\n}\r\n\r\n.p_place_dict {\r\n  color: rgb(0, 0, 0);\r\n  font-weight: bold;\r\n  font-size: 1.2rem;\r\n  color: rgb(46, 46, 155);\r\n}\r\n.registration {\r\n  display: flex;\r\n  align-items: center;\r\n  gap: 30px;\r\n}\r\n.one_sec2 {\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n\r\n.h4_name_sec2 {\r\n  color: rgb(46, 46, 155);\r\n  font-weight: bold;\r\n  font-size: 20px;\r\n  margin-top: 20px;\r\n  width: 100%;\r\n}\r\n.name_sec2 {\r\n  border-left: none;\r\n  border-right: none;\r\n  border-top: none;\r\n  border-bottom: 1px solid black;\r\n  padding: 13px 0px 4px 10px;\r\n  width: 100%;\r\n}\r\n\r\n.a {\r\n  color: rgb(46, 46, 155);\r\n  width: 30%;\r\n  font-weight: bold;\r\n  font-size: 20px;\r\n  margin-top: 20px;\r\n}\r\n.pare {\r\n  display: flex;\r\n  flex-direction: row;\r\n}\r\n\r\n.parent {\r\n  border-left: none;\r\n  border-right: none;\r\n  border-top: none;\r\n  border-bottom: 1px solid black;\r\n  padding: 13px 0px 4px 10px;\r\n  width: 50%;\r\n}\r\n\r\n.weight_sex {\r\n  display: flex;\r\n  align-items: center;\r\n  gap: 70px;\r\n}\r\n\r\n/* ...................footer */\r\n.footer {\r\n  display: flex;\r\n  flex-direction: column;\r\n  margin-left: 90px;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.footer h1 {\r\n  color: rgb(46, 46, 155);\r\n  font-weight: bold;\r\n  font-size: 2rem;\r\n}\r\n\r\n.act {\r\n  color: rgb(46, 46, 155);\r\n}\r\n\r\n.saxiix {\r\n  display: flex;\r\n}\r\n.saxi_text {\r\n  margin-top: 20px;\r\n  font-weight: bold;\r\n  font-size: 20px;\r\n  color: rgb(46, 46, 155);\r\n}\r\n.saxi_line {\r\n  border-left: none;\r\n  border-right: none;\r\n  border-top: none;\r\n  border-bottom: 1px solid black;\r\n  padding: 13px 0px 4px 10px;\r\n  width: 30%;\r\n  margin-left: 20px;\r\n}\r\n\r\n/* ....................... page breaks */\r\n@media print {\r\n  .page_break {\r\n    page-break-before: always;\r\n  }\r\n}\r\n",
