  "html": "  {% if letter_head and not no_letterhead -%}\r\n   <div class=\"flex items-start justify-between\">\r\n        <div class=\"letter-head text-center mb-5\">{{ letter_head }}</div>\r\n        <!--Contact info-->\r\n    <!--<div>-->\r\n    <!--    <div class=\"main_one\">-->\r\n    <!--        <div class=\"inner_one\">-->\r\n    <!--            <h4 class=\"text-lg\"><strong>Address:</strong></h4>-->\r\n    <!--        </div>-->\r\n    <!--        <div class=\"inner_two\">-->\r\n    <!--            <h4 class=\"text-lg\">Hodan</h4>-->\r\n    <!--        </div>-->\r\n    <!--    </div>-->\r\n    <!--</div>-->\r\n   </div>\r\n \r\n  {%- endif %}\r\n\r\n  {% if (doc.docstatus != 2) %}\r\n  <section>\r\n        \r\n    <div class=\"main\">\r\n          <div class = \"info\">\r\n        <div class=\"main_one\">\r\n          \r\n            <div class=\"inner_one\">\r\n                <h4 class=\"text-lg\"><strong>Patient Name:</strong></h4>\r\n                <h4 class=\"text-lg\"><strong>Patient ID:</strong></h4>\r\n                <h4 class=\"text-lg\"><strong>Age:</strong></h4>\r\n                 <h4 class=\"text-lg\"><strong>Sex:</strong></h4>\r\n                <h4 class=\"text-lg\"><strong>Tel No:</strong></h4>\r\n                \r\n            </div>\r\n            <div class=\"inner_two\">\r\n                <h4 class=\"text-lg\">{{doc.patient_name}}</h4>\r\n                <h4 class=\"text-lg\">{{doc.patient}}</h4>\r\n                <h4 class=\"text-lg\">{{doc.patient_age}}</h4>\r\n                <h4 class=\"text-lg\">{{frappe.db.get_value(\"Patient\" , doc.patient , \"sex\") or \"\"}}</h4>\r\n                <h4 class=\"text-lg\">{{frappe.db.get_value(\"Patient\" , doc.patient , \"mobile_no\")}}</h4>\r\n               \r\n            </div>\r\n\r\n        </div>\r\n        <div class=\"main_two\">\r\n            <div class=\"inner_one\">\r\n                    <h4 class=\"text-lg\"><strong>Lab Ref No:</strong></h4>\r\n                    <h4 class=\"text-lg\"><strong>Report Date:</strong></h4>\r\n                    <h4 class=\"text-lg\"><strong>Report Time:</strong></h4>\r\n                    <h4 class=\"text-lg\"><strong>Doctor Name:</strong></h4>\r\n            </div>\r\n            <div class=\"inner_two\">\r\n                    <h4 class=\"text-lg\">{{doc.lab_ref or \"\"}}</h4>\r\n                    <h4 class=\"text-lg\">{{doc.date}} {{frappe.format(doc.time_collected  , \"Time\" ) or \"\"}}</h4>\r\n                    <h4 class=\"text-lg\">{{frappe.format(doc.time  , \"Time\" ) or \"\"}}</h4>\r\n                     <h4 class=\"text-lg\">{{ doc.practitioner }}</h4>\r\n            </div>\r\n        </div>\r\n        </div>\r\n    </div>\r\n</section>\r\n<!--new one-->\r\n\r\n    <section  class=\"flex flex-col\">\r\n      <h1\r\n        class=\" text-red-700 text-start font-bold uppercase mt-1\"\r\n      >\r\n        {{doc.template}} Report\r\n      </h1>\r\n\r\n      <div class=\"flex flex-col\">\r\n    <!--Header-->\r\n        <div style=\"background-color: rgb(59, 130, 246); color: white; padding: 2px\" class=\"flex items-center justify-between mt-[2px] bg-blue-500 text-white\">\r\n            <span class=\"text-white flex-1 cus_cl \">Test name</span>\r\n             <span class=\"text-white flex-1 cus_cl \" >Result </span>\r\n             <span class=\"text-white flex-1 cus_cl\">Flag </span>\r\n             <span class=\"text-white flex-1 cus_cl \" >UOM </span>\r\n             <span class=\"text-white flex-1 cus_cl\" >N Range </span>\r\n        </div>\r\n          \r\n    <!-- Test Items -->\r\n        <div class=\"flex flex-col\">\r\n             {% set arrow = \"\" %}\r\n            \r\n                 {% if doc.template == \"CBC\" %}\r\n                 \r\n               <div\r\n               style=\"background-color: rgb(59, 130, 246); color: black; padding: 2px\"\r\n            class=\"flex items-center justify-between mt-[2px]  bg-blue-500 text-white\"\r\n          >\r\n            <span class=\"text-blue-800 flex-1 cus_cl \">Prameter</span>\r\n             <span class=\"text-blue-800 flex-1 cus_cl\" style= \"\">Result</span>\r\n               <span class=\"flex-1 cus_cl\">UOM</span>\r\n            <span class=\"text-blue-800 flex-1 cus_cl\">N Range</span>\r\n          </div>\r\n            {% endif %}\r\n        {% for test in doc.normal_test_items %}\r\n \r\n     \r\n{% if test.normal_range  %}\r\n{% if test.result_value and   '-' in test.normal_range  %}\r\n\r\n    {% set normal_parts = test.normal_range.split('-') %}\r\n    \r\n    {% if normal_parts|length == 2 %}\r\n        {% set down_normal = normal_parts[0] | float %}\r\n        {% set up_normal = normal_parts[1] | float %}\r\n        {% set result = test.result_value | float %}\r\n        \r\n        {% if result < down_normal %}\r\n            {% set arrow = '<span class=\"text-blue-800\" style=\"font-size: 14px; font-weight: lighter;\">L </span>' %}\r\n        {% elif result > up_normal %}\r\n            {% set arrow = '<span class=\"text-blue-800\" style=\"font-size: 14px; font-weight: lighter;\">H </span>' %}\r\n        {% endif %}\r\n    {% endif %}\r\n    \r\n{% endif %}\r\n{% endif %}\r\n\r\n    <!-- Test Row -->\r\n            {% if test.lab_test_name %}\r\n          <div\r\n            class=\"flex items-center justify-between mt-[2px]\"\r\n          >\r\n            <span class=\"text-blue-800 flex-1 \">{{test.lab_test_name}}</span>\r\n            <span class=\"text-blue-800 flex-1\" >{{test.result_value or \"\"}}</span>\r\n <span class=\"text-blue-800 flex-1 \">{{ arrow or \"\" }}</span>\r\n\r\n             <span class=\"text-blue-800 flex-1 \" >{{test.lab_test_uom or \"\"}}</span>\r\n             <span class=\"text-blue-800 flex-1\">{{test.normal_range or \"\"}}</span>\r\n          </div>\r\n            {% endif %}\r\n            \r\n    <!-- Event Row (if any) -->\r\n            {% if test.lab_test_event %}\r\n          <div class=\"flex flex-col mt-1 \">\r\n            <div class=\"flex items-center justify-between\" >\r\n              <span class=\"flex-1\">{{ test.lab_test_event}}</span>\r\n              <span class=\"flex-1\">\r\n                  {{test.result_value or ''}}\r\n              </span>\r\n <span class=\"flex-1 \">{{ arrow or \"\" }}</span>\r\n\r\n              <span class=\"flex-1 \">{{test.lab_test_uom or ''}}</span>\r\n              <span class=\"flex-1 \">{{test.normal_range or ''}}</span>\r\n            </div>\r\n       \r\n          </div>\r\n               {% endif %}\r\n        {% endfor %}\r\n        </div>\r\n\r\n      </div>\r\n    </section>\r\n    <br>\r\n                      <span class=\"border red text-center w-full text-red font-bold\"> {{doc.wbc_message or ''}}</span>\r\n                       <span class=\"border border-gray-400 text-center w-full text-black font-bold\">{{doc.rbc_message or ''}}</span>\r\n                        <span class=\"border red text-center w-full text-black font-bold\">{{doc.plt_message or ''}}</span>\r\n\r\n   <!--<footer class=\"mx-12 flex flex-col gap-2 mt-3\">-->\r\n   <!--   <div class=\"flex items-center gap-2\">-->\r\n   <!--     <span class=\"self-center\">Signature:- </span>-->\r\n   <!--     <span class=\"bg-black w-36 h-[1px] self-end\"></span>-->\r\n   <!--   </div>-->\r\n   <!--   <span class=\"bg-green-700 text-white px-3 mt-5\"-->\r\n   <!--     >Contact Us: 615xxxxxx  Email: info@hodanhospita.com    web: hodanhospita.com </span-->\r\n   <!--   >-->\r\n   <!-- </footer>-->\r\n    {% endif %}\r\n\r\n",
