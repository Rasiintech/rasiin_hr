  "html": "<!DOCTYPE html>\r\n<html lang=\"en\">\r\n<head>\r\n    <meta charset=\"UTF-8\">\r\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\r\n    <title>Hodan hospital</title>\r\n    <!-- ..... css link -->\r\n    <link rel=\"stylesheet\" href=\"style.css\">\r\n    <!-- ......fontawseme -->\r\n    <link rel=\"stylesheet\" href=\"https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css\">\r\n</head>\r\n<body>\r\n    <section>\r\n        <!-- ........logo -->\r\n        <div class=\"main_logo\">\r\n            <div class=\"logo_left\">\r\n                 <img src=\"/files/logo hodan.png\" alt=\"\">\r\n            </div>\r\n            <div class=\"right_logo\">\r\n                <h2>PATIENT PROCEDURE</h2>\r\n                <h4>PRESCRIPTION</h4>\r\n            </div>\r\n        </div>\r\n        <!-- ........info -->\r\n        <div class=\"mian_info\">\r\n            <div class=\"info_1\">\r\n                <div class=\"info_id\">\r\n                    <h3>ID: {{doc.patient}}</h3>\r\n                    <!--<h4></h3>-->\r\n                </div>\r\n                <div class=\"info_age\">\r\n                    <h3>Age:{{frappe.db.get_value(\"Patient\",doc.patient,\"p_age\")}}<</h3>\r\n                    <!--<h4>/h3>-->\r\n                </div>\r\n                <div class=\"info_doctor\">\r\n                    <h3>Doctor:</h3>\r\n                    <h4>{{doc.practitioner_name}}</h3>\r\n                </div>\r\n            </div>\r\n            <div class=\"info_2\">\r\n                <div class=\"info_id_2\">\r\n                    <h3>Name: {{doc.patient_name}}</h3>\r\n                    <!--<h4></h3>-->\r\n                </div>\r\n                <div class=\"info_age_2\">\r\n                    <h3>sex: {{doc.patient_sex}}</h3>\r\n                    <!--<h4 class=\"male\"></h3>-->\r\n                </div>\r\n                <div class=\"info_doctor_2\">\r\n                    <h3>Date: {{doc.encounter_date}}</h3>\r\n                    <!--<h4 class=\"date\"></h3>-->\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </section>\r\n\r\n    <!-- ........ table -->\r\n    <section class=\"table_section\">\r\n        <table>\r\n            <thead>\r\n                <tr>\r\n                    <th>No</th>\r\n                    <th>PROCEDURE</th>\r\n                    <th>DEPARTMENT</th>\r\n                    \r\n                </tr>\r\n            </thead>\r\n\r\n            <tbody>\r\n                {% for pro in doc.procedure_prescription %}\r\n                    <tr>\r\n                        <td>{{pro.idx}}</td>\r\n                        <td class=\"drug\">{{pro.procedure}}</td>\r\n                        <td>{{pro.department}}</td>\r\n                        \r\n                    </tr>\r\n                    <!--<tr>-->\r\n                    <!--    <td>1</td>-->\r\n                    <!--    <td>somaliya soomaali baa iska leh</td>-->\r\n                    <!--    <td>BOX</td>-->\r\n                    <!--    <td>3</td>-->\r\n                    <!--    <td>1</td>-->\r\n                    <!--    <td>somaliya soomaali baa iska leh</td>-->\r\n                    <!--    <td></td>-->\r\n                    <!--</tr>-->\r\n                {% endfor %}\r\n            </tbody>\r\n        </table>\r\n    </section>\r\n\r\n    <!-- ......footer --> \r\n    <section>\r\n        <div class=\"signature\">\r\n            <h3>Signature: ________________________</h3>\r\n        </div>\r\n        <div class=\"bukanka\">\r\n            <h3>Soo Laabashada Bunkaanka Waa  <span class=\"days\">10 </span>Maalin Guahooda.</h3>\r\n        </div>\r\n        <div class=\"address\">\r\n             <h4>\r\n                <span><i class=\"fa fa-map-marker\"></i></span>\r\n                Hodan District, Banadir ,Abdikasim Road\r\n                <span><i class=\"fa fa-globe\"></i></span>\r\n                www.hodanhospital.so\r\n                <span><i class=\"fa fa-envelope\"></i></span>\r\n                info@hodanhospital.so\r\n                <span><i class=\"fa fa-phone\"></i></span>\r\n                0610000000\r\n            </h4>\r\n\r\n        </div>\r\n    </section>\r\n</body>\r\n</html>",
