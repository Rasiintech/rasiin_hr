  "html": "{% if letter_head and not no_letterhead -%}\r\n    <div class=\"letter-head text-center\" style = \"margin-left:10%; margin-right:10%\">{{ letter_head }}</div>\r\n \r\n  {%- endif %}\r\n\r\n  {% if (doc.docstatus != 2) %}\r\n<!--<h4 class=\"text-uppercase text-center\"><b><u>Department of {{ doc.requesting_department }}</u></b></h4>-->\r\n\t\r\n\r\n\r\n    <!--<section class=\"flex flex-col mx-12\" style = \"margin-top:0px\">-->\r\n    <!--  <div class=\"flex items-center \">-->\r\n    <!--    <span-->\r\n    <!--      class=\"border border-gray-400 text-center w-full text-black font-bold\"-->\r\n    <!--      >Name</span-->\r\n    <!--    >-->\r\n    <!--    <span-->\r\n    <!--      class=\"border border-gray-400 text-center w-full text-black font-bold\"-->\r\n    <!--      >{{doc.patient_name}}</span-->\r\n    <!--    >-->\r\n    <!--    <span-->\r\n    <!--      class=\"border border-gray-400 text-center w-full text-black font-bold\"-->\r\n    <!--      >Patient ID</span-->\r\n    <!--    >-->\r\n    <!--    <span-->\r\n    <!--      class=\"border border-gray-400 text-center w-full text-black font-bold\"-->\r\n    <!--      >{{doc.patient}}</span-->\r\n    <!--    >-->\r\n    <!--  </div>-->\r\n    <!--  <div class=\"flex items-center gap-5\">-->\r\n    <!--    <span-->\r\n    <!--      class=\"border border-gray-400 text-center w-full text-black font-bold\"-->\r\n    <!--      >Sex / Age</span-->\r\n    <!--    >-->\r\n    <!--    <span-->\r\n    <!--      class=\"border border-gray-400 text-center w-full text-black font-bold\"-->\r\n    <!--      >{{ frappe.db.get_value(\"Patient\",doc.patient,\"sex\") or \"\" }} / {{doc.patient_age}}  {{ frappe.db.get_value(\"Patient\",doc.patient,\"age_type\") or \"\" }}</span-->\r\n    <!--    >-->\r\n    <!--    <span-->\r\n    <!--      class=\"border border-gray-400 text-center w-full text-black font-bold\"-->\r\n    <!--      >Date</span-->\r\n    <!--    >-->\r\n    <!--    <span-->\r\n    <!--      class=\"border border-gray-400 text-center w-full text-black font-bold\"-->\r\n    <!--      >{{doc.date}}</span-->\r\n    <!--    >-->\r\n    <!--  </div>-->\r\n    <!--  <div class=\"flex items-center gap-5\">-->\r\n    <!--    <span-->\r\n    <!--      class=\"border border-gray-400 text-center w-full text-black font-bold\"-->\r\n    <!--      >Doctor</span-->\r\n    <!--    >-->\r\n    <!--    <span-->\r\n    <!--      class=\"border border-gray-400 text-center w-full text-black font-bold\"-->\r\n    <!--      >-->\r\n    <!--        {% if doc.practitioner %}-->\r\n    <!--       {{ doc.practitioner }}-->\r\n    <!--       {% else %}-->\r\n    <!--       {% if doc.patient_sex == \"Female\" %}-->\r\n    <!--       Herself -->\r\n    <!--       {% else %}-->\r\n    <!--       Himself-->\r\n    <!--        {% endif %}-->\r\n    <!--        {% endif %}-->\r\n    <!--    </span-->\r\n    <!--    >-->\r\n    <!--    <span-->\r\n    <!--      class=\"border border-gray-400 text-center w-full text-black font-bold\"-->\r\n    <!--      >Time</span-->\r\n    <!--    >-->\r\n    <!--    <span-->\r\n    <!--      class=\"border border-gray-400 text-center w-full text-black font-bold\"-->\r\n    <!--      >{{ frappe.format(doc.time  , \"Time\" ) or \"\"}} </span-->\r\n          \r\n                 \r\n\r\n    <!--    >-->\r\n    <!--  </div>-->\r\n    <!--</section>-->\r\n    <section>\r\n  <div style=\"border: 1px solid gray; padding: 10px; width: 100%;\">\r\n    <div style=\"display: flex; align-items: start; gap: 180px;\">\r\n      <div style=\"display: flex; align-items: center; gap: 10px;\">\r\n        <div>\r\n          <h4 style=\"color: rgb(53, 53, 53); padding: 10px 0 0 0;\"><strong>Patient Name:</strong></h4>\r\n          <h4 style=\"color: rgb(53, 53, 53); padding: 10px 0 0 0;\"><strong>Patient ID:</strong></h4>\r\n          <h4 style=\"color: rgb(53, 53, 53); padding: 10px 0 0 0;\"><strong>Age:</strong></h4>\r\n          <h4 style=\"color: rgb(53, 53, 53); padding: 10px 0 0 0;\"><strong>Sex:</strong></h4>\r\n          <h4 style=\"color: rgb(53, 53, 53); padding: 10px 0 0 0;\"><strong>Tel No:</strong></h4>\r\n        </div>\r\n        <div>\r\n          <h4 style=\"color: rgb(53, 53, 53); padding: 10px 0 0 0;\">{{doc.patient_name}}</h4>\r\n          <h4 style=\"color: rgb(53, 53, 53); padding: 10px 0 0 0;\">{{doc.patient}}</h4>\r\n          <h4 style=\"color: rgb(53, 53, 53); padding: 10px 0 0 0;\">{{doc.patient_age}}</h4>\r\n          <h4 style=\"color: rgb(53, 53, 53); padding: 10px 0 0 0;\">{{frappe.db.get_value(\"Patient\" , doc.patient , \"sex\") or \"\"}}</h4>\r\n          <h4 style=\"color: rgb(53, 53, 53); padding: 10px 0 0 0;\">{{frappe.db.get_value(\"Patient\" , doc.patient , \"mobile_no\")}}</h4>\r\n        </div>\r\n      </div>\r\n\r\n      <div style=\"display: flex; align-items: center;\">\r\n        <div>\r\n          <h4 style=\"color: rgb(53, 53, 53); padding: 10px 0 0 0;\"><strong>Lab Ref No:</strong></h4>\r\n          <h4 style=\"color: rgb(53, 53, 53); padding: 10px 0 0 0;\"><strong>Report Date:</strong></h4>\r\n          <h4 style=\"color: rgb(53, 53, 53); padding: 10px 0 0 0;\"><strong>Report Time:</strong></h4>\r\n          <h4 style=\"color: rgb(53, 53, 53); padding: 10px 0 0 0;\"><strong>Doctor Name:</strong></h4>\r\n        </div>\r\n        <div>\r\n          <h4 style=\"color: rgb(53, 53, 53); padding: 10px 0 0 0;\">{{doc.lab_ref or \"\"}}</h4>\r\n          <h4 style=\"color: rgb(53, 53, 53); padding: 10px 0 0 0;\">{{doc.date}} {{frappe.format(doc.time_collected  , \"Time\" ) or \"\"}}</h4>\r\n          <h4 style=\"color: rgb(53, 53, 53); padding: 10px 0 0 0;\">{{frappe.format(doc.time  , \"Time\" ) or \"\"}}</h4>\r\n          <h4 style=\"color: rgb(53, 53, 53); padding: 10px 0 0 0;\">{{ doc.practitioner }}</h4>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</section>\r\n\r\n\r\n    <section class=\"flex flex-col mx-12\">\r\n      <h1\r\n        class=\"border border-black text-red-700 text-center font-bold uppercase mt-1\"\r\n      >\r\n        {{doc.template}} Report\r\n      </h1>\r\n\r\n      <div class=\"flex flex-col\">\r\n          \r\n        <div class=\"flex flex-col\">\r\n             {% set arrow = \"\" %}\r\n            \r\n                 {% if doc.template == \"CBC\" %}\r\n                 \r\n               <div\r\n            class=\"flex items-center justify-between border border-gray-700 mt-[2px] font-bold\"\r\n          >\r\n            <span class=\"text-blue-800 flex-1 \">Prameter</span>\r\n             <span class=\"text-blue-800 flex-1 ml-3\" style= \"margin-left:70px !important\">Result</span>\r\n               <span class=\"flex-1 text-center\">Unit</span>\r\n            <span class=\"text-blue-800 text-center flex-1\">Range</span>\r\n          </div>\r\n            {% endif %}\r\n        {% for test in doc.normal_test_items %}\r\n          {% set arrow = \"\" %}\r\n        {% if test.result_value %}\r\n           \r\n           {% if doc.template == \"CBC\" %}\r\n  {% if test.normal_range and '~' in (test.normal_range | string) %}\r\n    {% set normal_raw = (test.normal_range | string).replace(' ', '') %}\r\n    {% set normal = normal_raw.split('~') %}\r\n    {% if normal | length > 1 %}\r\n      {% set down_normal = normal[0] | float %}\r\n      {% set up_normal = normal[1] | float %}\r\n      {% set result = (test.result_value or 0) | float %}\r\n      {% if result < down_normal %}\r\n        {% set arrow = '<span style=\"font-size: 18px; color: #f00\">&darr;</span>' %}\r\n      {% elif result > up_normal %}\r\n        {% set arrow = '<span style=\"font-size: 24px; color: #f00\">&uarr;</span>' %}\r\n      {% endif %}\r\n    {% endif %}\r\n  {% endif %}\r\n{% endif %}\r\n{% endif %}\r\n\r\n       \r\n            {% if test.lab_test_name %}\r\n          <div\r\n            class=\"flex items-center justify-between border border-gray-700 mt-[2px] font-bold\"\r\n          >\r\n            <span class=\"text-blue-800 flex-1 \">{{test.lab_test_name}}</span>\r\n             <span class=\"text-blue-800 flex-1 ml-5\" style= \"margin-left:145px !important\">Result</span>\r\n              <span class=\"flex-1 text-center\">Unit</span>\r\n            <span class=\"text-blue-800 text-center flex-1\">Range</span>\r\n          </div>\r\n            {% endif %}\r\n              {% if test.lab_test_event %}\r\n          <div class=\"flex flex-col mt-1 \">\r\n            <div class=\"flex items-center justify-between w-[900px] font-bold\" >\r\n              <span class=\"flex-1\"><strong>{{ test.lab_test_event}}</strong></span>\r\n              <span class=\"flex-1 text-right \"></span>\r\n              <span class=\"flex-1 text-left\" style = \"margin-left:-90px\"\r\n                ><strong>{{test.result_value or ''}}{{arrow}}</strong></span\r\n              >\r\n                      <span class=\"flex-1 text-center\"><strong>{{test.lab_test_uom or ''}}</strong></span>\r\n              <span class=\"flex-1 text-center\"><strong>{{test.normal_range or ''}}</strong></span>\r\n            </div>\r\n       \r\n          </div>\r\n               {% endif %}\r\n        {% endfor %}\r\n        </div>\r\n\r\n      </div>\r\n    </section>\r\n    <br>\r\n                      <span class=\"border red text-center w-full text-red font-bold\"> {{doc.wbc_message or ''}}</span>\r\n                       <span class=\"border border-gray-400 text-center w-full text-black font-bold\">{{doc.rbc_message or ''}}</span>\r\n                        <span class=\"border red text-center w-full text-black font-bold\">{{doc.plt_message or ''}}</span>\r\n\r\n   <!--<footer class=\"mx-12 flex flex-col gap-2 mt-3\">-->\r\n   <!--   <div class=\"flex items-center gap-2\">-->\r\n   <!--     <span class=\"self-center\">Signature:- </span>-->\r\n   <!--     <span class=\"bg-black w-36 h-[1px] self-end\"></span>-->\r\n   <!--   </div>-->\r\n   <!--   <span class=\"bg-green-700 text-white px-3 mt-5\"-->\r\n   <!--     >Contact Us: 615xxxxxx  Email: info@hodanhospita.com    web: hodanhospita.com </span-->\r\n   <!--   >-->\r\n   <!-- </footer>-->\r\n    {% endif %}\r\n",
