  "html": "<!DOCTYPE html> \r\n<html lang=\"en\">\r\n<head>\r\n\r\n    <!-- ..... css link -->\r\n    <link rel=\"stylesheet\" href=\"style.css\">\r\n    <!-- ......fontawseme -->\r\n    <link rel=\"stylesheet\" href=\"https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css\">\r\n</head>\r\n<body> \r\n     {{ letter_head }}\r\n          <div style=\"border: 1px solid black; padding: 0 15px; width: 100%\">\r\n\t\t\t\t\t<div style=\"display: flex; align-items: center; justify-content: space-between;\">\r\n    \t\t\t\t\t <p style=\"flex: 1\" class=\"bold\">Patient: {{doc.patient}}</p> \r\n    \t\t\t\t\t<p style=\"flex: 1\" class=\"bold\">Patient Name: {{doc.patient_name}}</p> \r\n\t\t\t\t\t</div>\r\n\r\n                    <div style=\"display: flex; align-items: center; justify-content: space-between;\">\r\n                    <p style=\"flex: 1\" class=\"bold\"> {{ _(\"Age: \") }}   {{ frappe.db.get_value(\"Patient\" ,doc.patient , \"p_age\") }} {{ frappe.db.get_value(\"Patient\" ,doc.patient , \"age_type\") }} </p> \r\n                   <p style=\"flex: 1\" class=\"bold\">{{ _(\"Mobile: \") }} {{ frappe.db.get_value(\"Patient\" ,doc.patient , \"mobile_no\") }}</p> \r\n                    </div>\r\n                    \r\n                    <div style=\"display: flex; align-items: center; justify-content: space-between;\">\r\n                      \r\n                    <p style=\"flex: 1\" class=\"bold\">{{ _(\"Doctor: \") }} {{ doc.practitioner }}</p> \r\n                    <p style=\"flex: 1\" class=\"bold\">{{ _(\"Department: \") }} {{ doc.medical_department or \"\"}}</p> \r\n                    </div>\r\n                    <div style=\"display: flex; align-items: center; justify-content: space-between;\">\r\n                      \r\n                    <p style=\"flex: 1\" class=\"bold\">{{ _(\"Customer Group: \") }} {{ doc.customer_group }}</p> \r\n                    </div>\r\n                    </div>\r\n    <section>\r\n        <!-- ........logo -->\r\n        <!--<div class=\"main_logo\">-->\r\n        <!--    <div class=\"logo_left\">-->\r\n        <!--       {{ letter_head }}-->\r\n        <!--    </div>-->\r\n        <!--    <div class=\"right_logo\">-->\r\n        <!--        <h2>PATIENT SERVICE</h2>-->\r\n        <!--        <h4>PRESCRIPTION</h4>-->\r\n        <!--    </div>-->\r\n        <!--</div>-->\r\n        <!-- ........info -->\r\n        <!--<div class=\"mian_info\">-->\r\n        <!--    <div class=\"info_1\">-->\r\n        <!--        <div class=\"info_id\">-->\r\n        <!--            <h3>ID: {{doc.patient}}</h3>-->\r\n                    <!--<h4></h3>-->\r\n        <!--        </div>-->\r\n        <!--        <div class=\"info_age\">-->\r\n        <!--            <h3>Age: {{frappe.db.get_value(\"Patient\",doc.patient,\"p_age\")}}</h3>-->\r\n                    <!--<h4></h3>-->\r\n        <!--        </div>-->\r\n        <!--        <div class=\"info_doctor\">-->\r\n        <!--            <h3>Doctor:{{doc.practitioner_name}}</h3>-->\r\n                    <!--<h4></h3>-->\r\n        <!--        </div>-->\r\n        <!--    </div>-->\r\n        <!--    <div class=\"info_2\">-->\r\n        <!--        <div class=\"info_id_2\">-->\r\n        <!--            <h3>Name: {{doc.patient_name}}</h3>-->\r\n                    <!--<h4></h3>-->\r\n        <!--        </div>-->\r\n        <!--        <div class=\"info_age_2\">-->\r\n        <!--            <h3>sex: {{doc.patient_sex}}</h3>-->\r\n                    <!--<h4 class=\"male\"></h3>-->\r\n        <!--        </div>-->\r\n        <!--        <div class=\"info_doctor_2\">-->\r\n        <!--            <h3>Date: {{doc.encounter_date}}</h3>-->\r\n                    <!--<h4 class=\"date\"></h3>-->\r\n        <!--        </div>-->\r\n        <!--    </div>-->\r\n        <!--</div>-->\r\n    </section>\r\n\r\n    <!-- ........ table -->\r\n    <section class=\"table_section\">\r\n        {% set ns = namespace(show_tooth=false) %}\r\n        {% for img in doc.services_prescription %}\r\n            {% if img.tooth %}\r\n                {% set ns.show_tooth = true %}\r\n            {% endif %}\r\n        {% endfor %}\r\n        \r\n        <table>\r\n            <thead>\r\n                <tr>\r\n                    <th style=\"color:black\">No</th>\r\n                    <th style=\"color:black\">Service</th>\r\n                    <th style=\"color:black\">Qty</th>\r\n                    {% if ns.show_tooth %}\r\n                        <th style=\"color:black\">Tooth</th>\r\n                    {% endif %}\r\n                    <th style=\"color:black\">Comments</th>\r\n                </tr>\r\n            </thead>\r\n        \r\n            <tbody>\r\n                {% for img in doc.services_prescription %}\r\n                    <tr>\r\n                        <td style=\"color:black\">{{ img.idx }}</td>\r\n                        <td class=\"drug\" style=\"color:black\">{{ img.service }}</td>\r\n                        <td style=\"color:black\">{{ img.qty }}</td>\r\n                        {% if ns.show_tooth %}\r\n                            <td class=\"drug\" style=\"color:black\">{{ img.tooth or '' }}</td>\r\n                        {% endif %}\r\n                        <td style=\"color:black\">{{ img.comments or '' }}</td>\r\n                    </tr>\r\n                {% endfor %}\r\n            </tbody>\r\n        </table>\r\n\r\n    </section>\r\n\r\n    <!-- ......footer -->\r\n    <!--<section>-->\r\n    <!--    <div class=\"signature\">-->\r\n    <!--        <h3>Signature: ________________________</h3>-->\r\n    <!--    </div>-->\r\n    <!--    <div class=\"bukanka\">-->\r\n            <!--<h3>Soo Laabashada Bunkaanka Waa  <span class=\"days\">10 </span>Maalin Guahooda.</h3>-->\r\n    <!--    </div>-->\r\n    <!--     <div class=\"address\">-->\r\n    <!--        <h4>-->\r\n    <!--            <span><i class=\"fa fa-map-marker\"></i></span>-->\r\n    <!--            Warta Nabadda District, Wadnaha Road,-->\r\n    <!--            <span><i class=\"fa fa-globe\"></i></span>-->\r\n    <!--            www.jazeerahospital.so-->\r\n    <!--            <span><i class=\"fa fa-envelope\"></i></span>-->\r\n    <!--            info@jazeerahospital.so-->\r\n    <!--            <span><i class=\"fa fa-phone\"></i></span>-->\r\n                <!--0611222265-->\r\n    <!--        </h4>-->\r\n\r\n    <!--    </div>-->\r\n    <!--</section>-->\r\n</body>\r\n</html>",
