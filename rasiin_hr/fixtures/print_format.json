\n  color: #342042;\r\n}\r\n\r\n/* ...........footer */\r\n.signature {\r\n  display: flex;\r\n  justify-content: center;\r\n  height: 100px;\r\n  align-items: center;\r\n  color: #342042;\r\n}\r\n.bukanka {\r\n  display: flex;\r\n  justify-content: center;\r\n  color: #3e30c2;\r\n  border-bottom: 2px dotted #1c146b;\r\n  padding: 10px 0px 10px 0px;\r\n}\r\n.days {\r\n  color: red;\r\n}\r\n.address {\r\n  display: flex;\r\n  justify-content: center;\r\n  padding-top: 20px;\r\n  color: #342042;\r\n}\r\n.address h4 {\r\n  font-size: 14px;\r\n}\r\n.address h4 span {\r\n  padding: 0px 5px 0px 5px;\r\n}\r\n\r\ntable th:nth-child(1),\r\ntable td:nth-child(1) {\r\n  width: 50px;\r\n  text-align: center;\r\n}\r\n\r\ntable th:nth-child(2),\r\ntable td:nth-child(2) {\r\n  width: auto; /* Let it take remaining space */\r\n  text-align: center; /* Center the PROCEDURE text */\r\n  word-wrap: break-word; /* Allow long words to wrap */\r\n}\r\n",
