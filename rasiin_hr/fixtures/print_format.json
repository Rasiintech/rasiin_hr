  "html": "\n<body>\n\n    <!-- .........section 1 -->\n    {{ letter_head }}\n    \n   <section>\n                         <table>\n                        <tr>\n                            \n                                <tr>\n                                    \n                                    <th>Patient Details:</th>\n                                    <th>Admission and GP Details</th>\n\n                                </tr>\n                                <tr>\n                                    <td>Name:<span> {{doc.patient_name}}</span></td> \n                                    <td>Discharging Consultant:<span> {{doc.ref_practitioner}}</span></td>\n                                </tr>\n                                   <tr>\n                                    <td>Sex:<span> {{doc.sex}}</span></td>\n                                    <td>Date of Admission<span> {{doc.date_of_admission}}</span>:</td>\n                                </tr>\n                                      <tr>\n                                    <td>Date of birth:<span> {{doc.date_of_birth}}</span></td>\n                                    <td>Date of Discharge:<span> {{doc.date}}</span> </td>\n                                </tr>\n                                <tr>\n                                    <td>Registration no.:<span>{{doc.registration_no}}</span></td>\n                                    <td>Time of discharge:<span> {{doc.time_of_discharge}}</span> </td>\n                                </tr>\n                                </table>\n                                <table class=\"table2\"> \n                                <tr>\n                                    \n                                    <th>Diagnosis at Discharge</th>\n                                    <th>Operations and Procedures </th>\n                                    </tr>\n                                    <td class=\"td\"><span> {{doc.diagnosis_at_discharge}}</span></td>\n                                    <td class=\"td\"><span> {{doc.operations_and_procedures}}</span></td>\n                                </table>\n                           \n\n                        \n                    </section>\n                    <section>\n                        \n                                <table class=\"table2\">\n                        <tr>\n                                <tr>\n                                    <th>Reason for Admission and Presenting Complaint(s):</th>\n                                    \n                                    \n\n                                </tr>\n                                <td class=\"td\">{{doc.reason__admission_and_presenting_complaints}}</td>\n                         \n                        </table>\n                        \n                    </section>\n               <section>\n                        <table class=\"table2\">\n                        <tr>\n                                <tr>\n                                    <th>Relevant Investigations and Results</th>\n\n                                </tr>\n                                <td class=\"td\">{{ doc.relevant_investigations_and_results }}</td>\n                        </table>\n                    </section>\n                    <section>\n                        <table>\n                        <tr>\n                            \n                                <tr>\n                                    \n                                    <th>Physical Ability & Cognitive Function</th>\n                                    <th>On Admission\tAt Discharge:</th>\n\n                                </tr>\n                                <td><span></span></td>\n                                 <td class=\"td\"> <span> {{doc.on_admission_at_discharge}}</span></td>\n                                <tr>\n                                    <td>Physical: <span> {{doc.physical}}</span></td>\n                                </tr>\n                                <tr>\n                                    <td>Cognitive:<span> {{ doc.cognitive}}</span></td>\n                                </tr>\n                                    <tr>\n                                    <td>Others:<span> {{ doc.others}}</span></td>\n                                </tr>\n                                <tr>\n                                    <th>Advice, recommendations and future plans (including results awaited and outstanding investigations):</th>\n                                    <td>G.P. Actions (Date) <span>{{  doc.gp_actions_date}}</span></td>\n                                </tr>\n                                </table>\n                                <table class=\"table2\">\n                        <tr>\n                                <tr>\n                                    <th>Actions and Outstanding Investigations:</th>\n\n                                </tr>\n                                <td class=\"td\">{{doc.actions_and_outstanding_investigations}}</td>\n                        </table>\n                            <table class=\"table2\"> \n                                <tr>\n                                    \n                                    <th>Medications Stopped/ Changed If yes please give details:</th>\n                                    <th>Yes/ No: {{  doc.select}}</th>\n                                    </tr>\n                                    <td class=\"td\"> <span> {{doc.if_yes_please_give_details}}</span></td>\n                                </table>\n                    \n                    </section>\n                    <section>\n                        <h1>Details of Discharging Doctor</h1>\n                        <h3>Name of Doctor:{{doc.name_of_doctor}}</h3>\n                        <h4>Doctors Signature...................................................................................................</h4>\n                        <h4>Date:{{doc.date}}</h4>\n                    </section>\n                    <section>\n                     <div class=\"discharge_medication\">Drug\n                        <table>\n                       \n                    \n                    \t\t<tr>\n                    \t\t\t<th>Discharge Medications</th>\n                    \t\t\t<th>Qty</th>\n                    \t\t\t<th>Frequency</th>\n                    \t\t\t<th>Route</th>\n                    \t\t</tr>\n                    \t\t\t{%- for row in doc.discharge_drug_prescription -%}\n                            <tr>\n                                <td>{{ row.drug_code }}</td>\n                    \t\t\t<td>{{ row.qty }}</td>\n                    \t\t\t<td>{{ row.dosage or \"\" }}</td>\n                    \t\t\t<td>{{ row.root_type }}</td>\n                    \t\t</tr>\n                    \t\t\t{%- endfor -%}\n                    \t\t\n                        </table>\n                        </div>\n                        \n                        <div class=\"table3\">\n                        <table>\n                       \n                        <tr>\n                            <td>Pharmacy dispensed by: <span>{{doc.pharmacy_dispensed_by}}</span></td>\n                            <td>Date: <span>{{doc.deet}}</span></td>\n                        </tr>\n                         </table>\n                         </div>\n                        </section>\n                        <section>\n                            <div>\n                                <h3>Finance department:</h3>\n                                <h5> state of patient account clearance</h5>\n                                <input type=\"checkbox\"/>Clearing\n                                   <input type=\"checkbox\"/>Not Clearing\n                                   <h3>IF NOT CLEARING (Residual fee)\u2026\u2026\u2026\u2026\u2026\u2026\u2026\u2026\u2026</h3>\n                                   <h3>Guarantor\u2026\u2026\u2026\u2026\u2026\u2026\u2026\u2026\u2026\u2026\u2026\u2026\u2026\u2026\u2026.</h3>\n                                   <h3>Date: <span>{{doc.datte}}</span></h3>\n                            </div>\n                        </section>\n                       \n                \n\n\n</body>\n",
