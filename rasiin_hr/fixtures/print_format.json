  "html": "<!DOCTYPE html>\r\n<html lang=\"en\">\r\n<head>\r\n    <meta charset=\"UTF-8\">\r\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\r\n    <title>Hodan hospital receipt Letter</title>\r\n    <link rel=\"stylesheet\" href=\"style.css\">\r\n      <!-- ......fontawseme -->\r\n      <link rel=\"stylesheet\" href=\"https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css\">\r\n</head>\r\n<body>\r\n    <section>\r\n        <!-- ........logo -->\r\n        <div class=\"main_logo\">\r\n            <div class=\"logo_left\">\r\n                <img src=\"/files/logo hodan.png\" alt=\"\">\r\n            </div>\r\n            <div class=\"verifed_icon\">\r\n                <span>\r\n                    <svg xmlns=\"http://www.w3.org/2000/svg\" width=\"1em\" height=\"1em\" viewBox=\"0 0 16 16\"><path fill=\"currentColor\" fill-rule=\"evenodd\" d=\"M8.716.315a1 1 0 0 0-1.432 0L6.646.97a1 1 0 0 1-.988.265l-.88-.248a1 1 0 0 0-1.24.716l-.226.886a1 1 0 0 1-.723.723l-.886.225a1 1 0 0 0-.716 1.24l.248.881a1 1 0 0 1-.265.988l-.655.638a1 1 0 0 0 0 1.432l.655.639a1 1 0 0 1 .265.987l-.248.88a1 1 0 0 0 .716 1.24l.886.226a1 1 0 0 1 .723.723l.225.886a1 1 0 0 0 1.24.717l.881-.248a1 1 0 0 1 .988.264l.638.655a1 1 0 0 0 1.432 0l.639-.655a1 1 0 0 1 .987-.264l.88.248a1 1 0 0 0 1.24-.717l.226-.886a1 1 0 0 1 .723-.723l.886-.225a1 1 0 0 0 .717-1.24l-.248-.88a1 1 0 0 1 .264-.988l.655-.639a1 1 0 0 0 0-1.432l-.655-.638a1 1 0 0 1-.264-.988l.248-.88a1 1 0 0 0-.717-1.24l-.886-.226a1 1 0 0 1-.723-.723l-.225-.886a1 1 0 0 0-1.24-.716l-.88.248A1 1 0 0 1 9.354.97zm3.057 5.975a.75.75 0 0 0-1.042-1.08L6.597 9.202L5.28 7.887A.75.75 0 0 0 4.22 8.95l1.839 1.834a.75.75 0 0 0 1.05.01z\" clip-rule=\"evenodd\"/></svg></span>\r\n            </div>\r\n            <div class=\"right_logo\">\r\n                <div class=\"recept\">\r\n                    <h2>RECEIPT</h2>\r\n                     <h2>LETTER</h2>\r\n                </div>\r\n                <div class=\"number\">\r\n                    <h1># 18</h1>\r\n                </div>\r\n            </div>\r\n        </div>\r\n        <!-- ........info -->\r\n        <div class=\"mian_info\">\r\n            <div class=\"info_1\">\r\n                <div class=\"info_id\">\r\n                    <h3>Date</h3>\r\n                    <h4>{{doc.posting_date}}</h4>\r\n                </div>\r\n                <div class=\"info_age\">\r\n                    <h3>Patient</h3>\r\n                    <h4>{{doc.patient_name}}</h4>\r\n                </div>\r\n                \r\n            </div>\r\n            <div class=\"info_2\">\r\n                <div class=\"info_id_2\">\r\n                    <h3>Reff #:</h3>\r\n                    <h4>{{doc.name}}</h4>\r\n                </div>\r\n                <div class=\"info_age_2\">\r\n                    <h3>Tel:</h3>\r\n                    <h4 class=\"male\">{{doc.mobile}}</h4>\r\n                </div>\r\n                <div class=\"info_doctor_2\">\r\n                    <h3>age:</h3>\r\n                    <h4 class=\"date\">{{frappe.db.get_value(\"Patient\", doc.patient, \"p_age\")}} </h4>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </section>\r\n\r\n\r\n    <!-- .table -->\r\n    <section class=\"table_section\">\r\n        <table>\r\n            <thead>\r\n                <tr>\r\n                    <th>#</th>\r\n                    <th>ITEM DESCRIPTION</th>\r\n                    <th>QUANTITY</th>\r\n                    <th>PRICE</th>\r\n                    <th>AMOUNT</th>\r\n                </tr>\r\n            </thead>\r\n            <tbody>\r\n                {% for item in doc.items %}\r\n                <tr>\r\n                    <td>{{ item.idx}}</td>\r\n                    <td>{{ item.item_code}}</td>\r\n                    <td>{{ item.qty | int}}</td>\r\n                    <td>{{ item.rate}}</td>\r\n                    <td>{{ item.amount}}</td>\r\n                </tr>\r\n                {% endfor %}\r\n            </tbody>\r\n        </table>\r\n    </section>\r\n\r\n    <!-- ........ subtotal -->\r\n    <section>\r\n        <!--<div class=\"finance_office\">-->\r\n        <!--    <h3>Finance Office</h3>-->\r\n        <!--    <h4>______________________________</h4>-->\r\n        <!--</div>-->\r\n        <div class=\"main_total\">\r\n            <div class=\"total\">\r\n                <h3>SubTotal</h3>\r\n                <h4>$ {{ doc.total}}</h4>\r\n            </div>\r\n            <div class=\"discount\">\r\n                <h3>Discount</h3>\r\n                <h4>$ {{doc.discount_amount}}</h4>\r\n            </div>\r\n            <div class=\"grand\">\r\n                <h3>Grand Total</h3>\r\n                <h4>$ {{ doc.total - doc.discount_amount}} </h4>\r\n            </div>\r\n            <div class=\"amount\">\r\n                <h3>Amount Paid</h3>\r\n                <h4>$ {{doc.paid_amount}}</h4>\r\n            </div>\r\n            <div class=\"balance\">\r\n                <h4>Balance: {{doc.outstanding_amount}}</h4>\r\n            </div>\r\n        </div>\r\n        \r\n    </section> \r\n         \r\n     \r\n    <!-- .address -->\r\n    <!--<section class=\"main_address\">-->\r\n    <!--     <div  class=\"thanks\">-->\r\n    <!--         <h4>Thank you for visiting our Hospital</h4>-->\r\n    <!--    </div>-->\r\n    <!--    <div class=\"address\">-->\r\n    <!--        <h4>-->\r\n    <!--            <span><i class=\"fa fa-map-marker\"></i></span>-->\r\n    <!--            Hodan District, Banadir ,Digfeer Road-->\r\n    <!--            <span><i class=\"fa fa-globe\"></i></span>-->\r\n    <!--            www.royalhospital.so-->\r\n    <!--            <span><i class=\"fa fa-envelope\"></i></span>-->\r\n    <!--            info@royalhospital.so-->\r\n    <!--            <span><i class=\"fa fa-phone\"></i></span>-->\r\n    <!--            0611222265-->\r\n    <!--        </h4>-->\r\n    \r\n    <!--    </div>-->\r\n    <!--</section>-->\r\n</body>\r\n</html>",
