  "html": "{% if letter_head %}\n    {{ letter_head }}\n{% endif %}\n\n<!--<p class=\"text-center\" style=\"margin-bottom: 1rem\">-->\n<!--\t{{ doc.company }}<br>-->\n<!--\t<b>{{ doc.select_print_heading or _(\"Invoice\") }}</b><br>-->\n<!--</p>-->\n\n\n<!--<div class=\"container\">-->\n<!--<div class=\"row\">-->\n<!-- <div  class=\"col-md-12\">-->\n     <span >\n        <strong class=\"mr-5\">{{ _(\"Name\") }}:</strong>\n        <strong>{{ doc.patient_name }}</strong>\n        <strong>{{ _(\"Age\") }}:{{ doc.age }}</strong>\n        <strong>{{ _(\"Request Practitoner\") }}:{{ doc.practitioner }}</strong>\n          <strong>{{ _(\"Lab Ref\") }}:{{ doc.lab_ref }}</strong>\n\n        <strong>{{ _(\"Patient ID\") }}:{{ doc.patient }}</strong>\n        <strong>{{ _(\"Report Date\") }}:{{ doc.date }}</strong>\n        <strong>{{ _(\"Exmination Date\") }}:{{ doc.date }}</strong>\n        </span>\n<!--    </div>-->\n    \n<!--</div>-->\n<!--</div>-->\n\n<hr>\n    {%set items = doc.get_lab_tests() %}\n\t\n\n\n\n{% for key , value in items.items() %}\n<h2 class=\"text-center font-weight-bold \">{{key}}</h2>\n<table class=\"table table-condensed\">\n\t<thead>\n\t\t<tr>\n\t\t\t<th width=\"20%\">{{ _(\"Test\") }}</th>\n\t\t\t<th width=\"45%\" c>{{ _(\"Test Name\") }}</th>\n\t\t\t<th width=\"20%\" >{{ _(\"Result\") }}</th>\n\t\t\t\t<th width=\"20%\" >{{ _(\"Normal Range\") }}</th>\n\t\t</tr>\n\t</thead>\n\t<tbody>\n{% for item in items[key] %}\n\n\t\n\t\t<tr>\n\t\t    {% if item.test %}\n\t\t\t<td>\n\t\t\t\t{{ item.test }}\n\t\t\t</td>\n\t\t\t{% else %}\n\t\t\t\t<td>\n\t\t\t\t\n\t\t\t</td>\n\t\t\t\n\t\t\t{% endif %}\n\t\t\t  {% if item.lab_test_name  or item.lab_event %}\n\t\t\n\t\t\t\t<td >{{item.lab_test_name}} <span class=\"m-5\">\t{{item.lab_event}}</span></td>\n\t\t\t\n\t\t\t\t\n\t\t\n\t\t\t\n\t\t\n\t\t\t\n\t\t\t{% else %}\n\t\t\t\t<td>\n\t\t\t\t\n\t\t\t</td>\n\t\t\t\n\t\t\t{% endif %}\n\t\t\t\n\t\t\t\n\t\t\t\n\t\t\t  {% if item.result_value %}\n\t\t\n\t\t\t\t<td >{{item.result_value}}</td>\n\t\t\n\t\t\t{% else %}\n\t\t\t\t<td>\n\t\t\t\t\n\t\t\t</td>\n\t\t\t\n\t\t\t{% endif %}\n\t\t\n\t    \n\t          {% if item.normal_range %}\n\t\t\n\t\t\t\t\t<td >{{item.normal_range}}</td>\n\t\t\n\t\t\t{% else %}\n\t\t\t\t<td>\n\t\t\t\t\n\t\t\t</td>\n\t\t\t\n\t\t\t{% endif %}\n\t\t\t\t\n\t\n\t\t\t\n\t\t</tr>\n\t\t{%- endfor -%}\n\t</tbody>\n</table>\n\n{% endfor %}\n\n<div class=\"container\">\n  <div class=\"row\">\n    <div class=\"col-md-3\">\n      One of three columns\n    </div>\n    <div class=\"col-md-3\">\n      One of three columns\n    </div>\n    <div class=\"col-md-3\">\n      One of three columns\n    </div>\n  </div>\n</div>\n<strong>Signature</strong>\n<div style = \"{width:  80% ; border : 1px solid #000}\">\n    ***END OF REPORT***\n</div>",
