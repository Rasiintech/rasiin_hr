  "css": "* {\r\n  padding: 0;\r\n  margin: 0;\r\n  box-sizing: border-box;\r\n}\r\n\r\n.main_logo {\r\n  display: flex;\r\n  justify-content: space-evenly;\r\n  align-items: center;\r\n  border-bottom: 1px solid gray;\r\n  margin: 0px 10px 0px 10px;\r\n}\r\n.logo_left img {\r\n  width: 300px;\r\n  height: 100px;\r\n}\r\n.right_logo h4 {\r\n  text-align: center;\r\n}\r\n\r\n/* .................. mian_info */\r\n.mian_info {\r\n  display: flex;\r\n  width: 100%;\r\n  justify-content: space-around;\r\n  padding-top: 10px;\r\n}\r\n.info_2 {\r\n  margin-left: 100px;\r\n}\r\n.info_id,\r\n.info_age,\r\n.info_doctor {\r\n  display: flex;\r\n}\r\n\r\n.info_id_2,\r\n.info_age_2,\r\n.info_doctor_2 {\r\n  display: flex;\r\n}\r\n/* ...... */\r\n.info_id h4 {\r\n  color: #9478a9;\r\n  padding-left: 100px;\r\n}\r\n.info_age h4 {\r\n  color: #9478a9;\r\n  padding-left: 90px;\r\n}\r\n.info_doctor h4 {\r\n  color: #9478a9;\r\n  padding-left: 64px;\r\n}\r\n/* ....... */\r\n.info_id_2 h4 {\r\n  color: #9478a9;\r\n  padding-left: 100px;\r\n}\r\n.info_age_2 h4 {\r\n  color: #9478a9;\r\n  padding-left: 127px;\r\n}\r\n.info_doctor_2 h4 {\r\n  color: #9478a9;\r\n  padding-left: 117px;\r\n}\r\n\r\n.info_id h3,\r\n.info_age h3,\r\n.info_doctor h3 {\r\n  /* color: rgb(56, 12, 100); */\r\n  color: #153b50;\r\n}\r\n\r\n/* ...........table_section */\r\n.table_section {\r\n  display: flex;\r\n  justify-content: center;\r\n  padding-top: 10px;\r\n  margin: 10px;\r\n}\r\ntable {\r\n  border-collapse: collapse;\r\n}\r\ntable th,\r\ntable td {\r\n  border-bottom: 1px solid gray;\r\n  border-top: 1px solid gray;\r\n  padding: 20px 0px 20px 0px;\r\n}\r\ntable th {\r\n  padding: 10px 30px 10px 30px;\r\n  text-align: left;\r\n  color: gray;\r\n  font-size: 14px;\r\n}\r\ntable th:nth-child(2) {\r\n  width: 350px;\r\n}\r\n\r\ntable th:nth-child(6) {\r\n  width: 250px;\r\n}\r\ntable td {\r\n  padding: 10px 30px 10px 30px;\r\n  text-align: left;\r\n  /* color: rgb(90, 71, 109); */\r\n  color: #342042;\r\n}\r\n\r\n/* ...........footer */\r\n.signature {\r\n  display: flex;\r\n  justify-content: center;\r\n  height: 100px;\r\n  align-items: center;\r\n  color: #342042;\r\n}\r\n.bukanka {\r\n  display: flex;\r\n  justify-content: center;\r\n  color: #3e30c2;\r\n  border-bottom: 2px dotted #1c146b;\r\n  padding: 10px 0px 10px 0px;\r\n}\r\n.days {\r\n  color: red;\r\n}\r\n.address {\r\n  display: flex;\r\n  justify-content: center;\r\n  padding-top: 20px;\r\n  color: #342042;\r\n}\r\n.address h4 {\r\n  font-size: 14px;\r\n}\r\n.address h4 span {\r\n  padding: 0px 5px 0px 5px;\r\n}\r\n\r\n\r\n",
