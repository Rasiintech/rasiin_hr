  "css": "* {\n  padding: 0;\n  margin: 0;\n  box-sizing: border-box;\n}\n\n/* ..........header__ */\n.header__ {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n}\n.header__ h3 {\n  font-size: 12px;\n  font-weight: 500;\n}\n.header__ h4 {\n  font-size: 12px;\n}\n.reciept_number {\n  display: flex;\n  padding: 0px 20px 0px 20px;\n}\n.reciept_number h3 {\n  font-weight: 800;\n}\n.queuue {\n  margin-left: 70px;\n}\n.logo img {\n  width: 100px;\n  height: 100px;\n}\n\n /*.... main_recpts*/\n.main_recpts {\n  margin-top: 20px;\n  display: flex;\n}\n.main_recpts h4 {\n  color: rgb(56, 55, 55);\n}\n.right_receipts {\n  margin-left: 60px;\n}\n.patient,\n.Mobile {\n  margin-top: 20px;\n}\n\n/* ........table_section */\n.table_section {\n  display: flex;\n  justify-content: center;\n  margin-top: 8px;\n}\ntable {\n  border-collapse: collapse;\n}\ntable th,\ntable td {\n  border-bottom: 1px solid black;\n  border-top: 1px solid black;\n   padding: 20px 0px 20px 0px; \n}\ntable th {\n   padding: 10px 30px 10px 30px; \n  padding: 4px;\n  text-align: left;\n  color: black;\n  font-weight: bold;\n  font-size: 14px;\n}\ntable td {\n  font-size: 13px;\n}\ntable th:nth-child(1) {\n   width: 350px; \n}\n\ntable td {\n  padding: 4px;\n   padding: 10px 30px 10px 30px; \n  text-align: left;\n   color: rgb(90, 71, 109); \n  color: #342042;\n}\n\n/* ..................main_subtotal */\n.main_subtotal {\n  width: 60%;\n  float: right;\n}\n.subtotal {\n  display: flex;\n  padding: 4px 20px 4px 20px;\n  color: rgb(75, 73, 73);\n  width: 300px;\n}\n.subtotal h3 {\n  padding-left: 30px;\n}\n.discount {\n  display: flex;\n  padding: 5px 20px 5px 20px;\n  width: 300px;\n  color: rgb(75, 73, 73);\n  border-bottom: 2px dotted rgb(33, 33, 33);\n}\n.discount h3 {\n  padding-left: 50px;\n}\n.total {\n  display: flex;\n  padding: 5px 20px 5px 20px;\n  width: 300px;\n  border-bottom: 2px dotted black;\n  color: rgb(43, 43, 43);\n  font-weight: bold;\n}\n.total h3 {\n  padding-left: 60px;\n}\n.paid {\n  display: flex;\n  padding: 5px 20px 5px 20px;\n  width: 300px;\n  border-bottom: 2px dotted black;\n  color: rgb(43, 43, 43);\n  font-weight: bold;\n}\n.paid h3 {\n  padding-left: 80px;\n}\n.balance {\n  display: flex;\n  padding: 5px 20px 5px 20px;\n  border-bottom: 2px dotted black;\n  width: 300px;\n  color: rgb(43, 43, 43);\n  font-weight: bold;\n}\n.balance h3 {\n  padding-left: 55px;\n}\n\nh1,\nh2,\nh3,\nh4,\nh5,\nh6{\n    padding: 0;\n    margin: 0;\n}\n\n\n/* ........mediacare */\n.mediacare{\n    /*margin-top: 200px;*/\n    display: flex;\n    flex-direction: column;\n    align-items: center;\n}\n.mediacare img{\n    width: 250px;\n    height: 40px;\n}\n.mediacare h3{\n    font-size: 12px;\n  color: rgb(77, 76, 76);\n}\n@media print{\n    \n.mediacare{\n    margin-top: 200px;\n    margin-left: 0px;\n    display: flex;\n    flex-direction: column;\n    align-items: center;\n    padding:0px;\n   \n}\n.main_subtotal {\n  width: 40%;\n  float: right;\n}\n}\n",
