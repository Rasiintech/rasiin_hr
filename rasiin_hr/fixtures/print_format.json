  "html": "\n\n<div class=\"container\" style = \"border: 1px dashed #000; border-bottom: none\">\n\n  {% if letter_head and not no_letterhead -%}\n   <div class=\"flex items-start justify-between\">\n        <div class=\"letter-head text-center mb-5\">{{ letter_head }}</div>\n        <!--Contact info-->\n    <!--<div>-->\n    <!--    <div class=\"main_one\">-->\n    <!--        <div class=\"inner_one\">-->\n    <!--            <h4 class=\"text-lg\"><strong>Address:</strong></h4>-->\n    <!--        </div>-->\n    <!--        <div class=\"inner_two\">-->\n    <!--            <h4 class=\"text-lg\">Hodan</h4>-->\n    <!--        </div>-->\n    <!--    </div>-->\n    <!--</div>-->\n   </div>\n \n  {%- endif %}\n\n  {% if (doc.docstatus != 2) %}\n<!--<h4 class=\"text-uppercase text-center\"><b><u>Department of {{ doc.requesting_department }}</u></b></h4>-->\n\t\n\n\n    <!--<section class=\"flex flex-col gap-1 \">-->\n    <!--  <div class=\"flex items-center gap-5\">-->\n    <!--    <span-->\n    <!--      class=\"border border-gray-400 text-center w-full text-black font-bold\"-->\n    <!--      >Name</span-->\n    <!--    >-->\n    <!--    <span-->\n    <!--      class=\"border border-gray-400 text-center w-full text-black font-bold\"-->\n    <!--      >{{doc.patient_name}}</span-->\n    <!--    >-->\n    <!--    <span-->\n    <!--      class=\"border border-gray-400 text-center w-full text-black font-bold\"-->\n    <!--      >Patient ID</span-->\n    <!--    >-->\n    <!--    <span-->\n    <!--      class=\"border border-gray-400 text-center w-full text-black font-bold\"-->\n    <!--      >{{doc.patient}}</span-->\n    <!--    >-->\n    <!--  </div>-->\n    <!--  <div class=\"flex items-center gap-5\">-->\n    <!--    <span-->\n    <!--      class=\"border border-gray-400 text-center w-full text-black font-bold\"-->\n    <!--      >Sex / Age</span-->\n    <!--    >-->\n    <!--    <span-->\n    <!--      class=\"border border-gray-400 text-center w-full text-black font-bold\"-->\n    <!--      >{{ frappe.db.get_value(\"Patient\",doc.patient,\"sex\") or \"\" }} / {{doc.patient_age}}  {{ frappe.db.get_value(\"Patient\",doc.patient,\"age_type\") or \"\" }}</span-->\n    <!--    >-->\n    <!--    <span-->\n    <!--      class=\"border border-gray-400 text-center w-full text-black font-bold\"-->\n    <!--      >Request Date</span-->\n    <!--    >-->\n    <!--    <span-->\n    <!--      class=\"border border-gray-400 text-center w-full text-black font-bold\"-->\n    <!--      >{{doc.date}}</span-->\n    <!--    >-->\n    <!--  </div>-->\n    <!--  <div class=\"flex items-center gap-5\">-->\n    <!--    <span-->\n    <!--      class=\"border border-gray-400 text-center w-full text-black font-bold\"-->\n    <!--      >Doctor:</span-->\n    <!--    >-->\n    <!--    <span-->\n    <!--      class=\"border border-gray-400 text-center w-full text-black font-bold\"-->\n    <!--      >-->\n    <!--        {% if doc.practitioner %}-->\n    <!--       {{ doc.practitioner }}-->\n    <!--       {% else %}-->\n    <!--       {% if doc.patient_sex == \"Female\" %}-->\n    <!--       Herself -->\n    <!--       {% else %}-->\n    <!--       Himself-->\n    <!--        {% endif %}-->\n    <!--        {% endif %}-->\n    <!--    </span-->\n    <!--    >-->\n    <!--    <span-->\n    <!--      class=\"border border-gray-400 text-center w-full text-black font-bold\"-->\n    <!--      >Print Date</span-->\n    <!--    >-->\n    <!--    <span-->\n    <!--      class=\"border border-gray-400 text-center w-full text-black font-bold\"-->\n    <!--      >{{ frappe.utils.getdate() }}</span-->\n    <!--    >-->\n    <!--  </div>-->\n    <!--</section>-->\n\n\n<!--kan cususb bilow-->\n\n    <section>\n        \n    <div class=\"main\">\n          <!--<div><span class=\"text-center text-lg\" ><strong>Patient Name :</strong> {{doc.patient_name}}</span></div>-->\n          <div class = \"info\">\n        <div class=\"main_one\">\n          \n            <div class=\"inner_one\">\n                <h4 class=\"text-lg\"><strong>Patient Name:</strong></h4>\n                <h4 class=\"text-lg\"><strong>Patient ID:</strong></h4>\n                <h4 class=\"text-lg\"><strong>Age:</strong></h4>\n                 <h4 class=\"text-lg\"><strong>Sex:</strong></h4>\n                <h4 class=\"text-lg\"><strong>Tel No:</strong></h4>\n                \n            </div>\n            <div class=\"inner_two\">\n                <h4 class=\"text-lg\">{{doc.patient_name}}</h4>\n                <h4 class=\"text-lg\">{{doc.patient}}</h4>\n                <h4 class=\"text-lg\">{{doc.patient_age}}</h4>\n                <h4 class=\"text-lg\">{{frappe.db.get_value(\"Patient\" , doc.patient , \"sex\") or \"\"}}</h4>\n                <h4 class=\"text-lg\">{{frappe.db.get_value(\"Patient\" , doc.patient , \"mobile_no\")}}</h4>\n               \n            </div>\n\n        </div>\n        <div class=\"main_two\">\n            <div class=\"inner_one\">\n                    <h4 class=\"text-lg\"><strong>Lab Ref No:</strong></h4>\n                    <h4 class=\"text-lg\"><strong>Report Date:</strong></h4>\n                    <h4 class=\"text-lg\"><strong>Report Time:</strong></h4>\n                    <h4 class=\"text-lg\"><strong>Doctor Name:</strong></h4>\n            </div>\n            <div class=\"inner_two\">\n                    <h4 class=\"text-lg\">{{doc.lab_ref or \"\"}}</h4>\n                    <h4 class=\"text-lg\">{{doc.date}} {{frappe.format(doc.time_collected  , \"Time\" ) or \"\"}}</h4>\n                    <h4 class=\"text-lg\">{{frappe.format(doc.time  , \"Time\" ) or \"\"}}</h4>\n                     <h4 class=\"text-lg\">{{ doc.practitioner }}</h4>\n            </div>\n        </div>\n        </div>\n    </div>\n</section>\n       \n       \n    \n<section>\n    {% set items = doc.get_lab_tests() %}\n    {% for key, value in items.items() %}\n        <div class=\"main_div\">\n            <div>{{ key }} Report</div>\n            <table class=\"mytable\">\n    <tr class=\"bg-blue-500 text-white\" style=\"color: white !important\">\n        <th class=\"px-4 text-lg cus_cl\" style=\"color: white\">Test Name</th>\n        <th class=\"px-4 text-lg cus_cl\" style=\"color: white\">Result</th>\n        <th class=\"px-4 text-lg cus_cl\" style=\"color: white\">Flag</th>\n        <th class=\"px-4 text-lg cus_cl\" style=\"color: white\">UOM</th>\n        <th class=\"px-4 text-lg cus_cl\" style=\"color: white\">N Range</th>\n    </tr>\n    {% for item in items[key] %}\n    <tr>\n        <td>{{ item.test }}</td>\n        <td>{{ item.result_value or '' }}</td>\n        <td>\n            {% set flag = item.flag or '' %}\n            {% set nr = item.normal_range or '' %}\n            {% set result = item.result_value | float(0) %}\n            \n            {% if not item.flag and nr %}\n                {% set nr_lower = nr.lower() %}\n                {% set range_text = nr.split(':')[-1] | trim %}\n                \n                <!-- Case 1: Standard or labeled range like \"30.0-100\", \"Adequacy: 30.0-100\" -->\n                {% if '-' in range_text %}\n                    {% set rest_f = range_text.split('-') %}\n                    {% if rest_f | length == 2 %}\n                        {% set low = rest_f[0] | float(0) %}\n                        {% set high = rest_f[1] | float(0) %}\n                        {% if result < low %}\n                            {% set flag = 'L' %}\n                        {% elif result > high %}\n                            {% set flag = 'H' %}\n                        {% else %}\n                            {% set flag = '' %}\n                        {% endif %}\n                    {% endif %}\n        \n                <!-- Case 2: \"up to 200\" -->\n                {% elif 'up to' in nr_lower %}\n                    {% set high_str = nr_lower.replace('up to', '') | trim %}\n                    {% set high = high_str | float(0) %}\n                    {% if result > high %}\n                        {% set flag = 'H' %}\n                    {% else %}\n                        {% set flag = '' %}\n                    {% endif %}\n                {% endif %}\n            {% endif %}\n        \n            {{ '' }}\n        </td>\n\n\n\n        <td>{{ frappe.db.get_value(\"Lab Test Template\", item.test, \"lab_test_uom\") or '' }}</td>\n        <td>{{ item.normal_range or '' }}</td>\n    </tr>\n    {% endfor %}\n</table>\n\n\n        </div>\n    {% endfor %}\n</section>\n\n\n\n<!--kan cusub dhamaad-->\n<!--  <section class=\"flex flex-col \">-->\n<!--         {%set items = doc.get_lab_tests() %}-->\n<!--        {% for key , value in items.items() %}-->\n<!--      <div class=\" font-bold text-xl text-center mt-3\">{{key}} Report</div>-->\n<!--     <div class=\"flex flex-col gap-1\">-->\n<!--  <div class=\"flex items-center bg-red-500 text-white font-bold\" style=\"border-bottom: 1px solid white;\">-->\n<!--    <span class=\"flex-1 flex items-center gap-1\" style=\"color:white;\">Test Name</span>-->\n<!--    <span class=\"flex-1 text-right pr-1\"></span>-->\n<!--    <span class=\"flex-1 text-center\" style=\"color:white;\">Result</span>-->\n<!--     <span class=\"flex-1 text-center\" style=\"color:white;\">Flag</span>-->\n<!--    <span class=\"flex-1 text-center\" style=\"color:white;\">UOM</span>-->\n<!--    <span class=\"flex-1 text-center\" style=\"color:white;\">N Range</span>-->\n     <!--<span class=\"flex-1 text-center\" style=\"color:white;\">Flag</span>-->\n<!--  </div>-->\n<!--  {% for item in items[key] %}-->\n<!--  <div class=\"flex items-center font-bold\" style=\"border-bottom: 1px solid black;\">-->\n<!--    <span class=\"flex-1 flex items-center gap-1\">-->\n<!--      <span class=\"h-2 w-2 bg-black rounded-full\"></span>-->\n<!--      {{ item.test }}-->\n<!--    </span>-->\n<!--    <span class=\"flex-1 text-right pr-1\"> </span>-->\n<!--    <span style=\"margin-left: 10px;\">{{ item.result_value or '' }}</span>-->\n<!--    <span style=\"margin-left: 10px;\">{{ item.flag or '' }}</span>-->\n<!--    <span style=\"margin-right: 35px;\" class=\"flex-1 text-right\">{{ frappe.db.get_value(\"Lab Test Template\", item.test, \"lab_test_uom\") or '' }}</span>-->\n<!--    <span class=\"flex-1 text-center\">{{ item.normal_range or '' }}</span>-->\n     <!--<span style=\"margin-left: 10px;\">{{ item.flag or '' }}</span>-->\n<!--  </div>-->\n<!--  {% endfor %}-->\n<!--</div>-->\n<!--    {% endfor %}-->\n<!--       </section>  -->\n       \n       \n       \n       \n    <!--<footer class=\"flex flex-col  relative mt-5\" >-->\n    <!--  <div class=\"flex items-center gap-14\">-->\n    <!--    <span class=\"text-white bg-green-700 font-bold\">Comment:</span>-->\n\n    <!--    <div class=\"flex flex-col mt-5\">-->\n    <!--      <span class=\"text-red-900 font-bold text-left underline\"-->\n    <!--        >{{doc.lab_test_comment or ''}}</span-->\n    <!--      >-->\n       \n    <!--    </div>-->\n    <!--    <div class=\"flex items-center gap-2-600 absolute right-0\">-->\n    <!--      <span>Signature: </span>-->\n    <!--      <span class=\"bg-black w-36 h-[1px]\"></span>-->\n    <!--    </div>-->\n    <!--  </div>-->\n\n  \n    <!--    <span class=\"bg-green-700 text-white px-3 mt-5\"-->\n    <!--    >Contact Us: 0613922224/0613922225/0613922226  Email: info@albarakathospital.com    web: albarakathospital.com  </span-->\n    <!--  >-->\n    <!--</footer>-->\n    <!--footerka cusub wayte-->\n    <!--    <div class=\"foter flex flex-col   mt-5\">-->\n        \n        \n    <!--      <div class=\"myfooter\">-->\n    <!--    <div class = \"child_footer\">-->\n    <!--            <div class=\"one\">-->\n    <!--        <h3>Reported by</h3>-->\n    <!--        <h3>____________________</h3>-->\n    <!--        <h4>{{frappe.get_fullname()}}</h4>-->\n    <!--        <h4>Laboratory Technician</h4>-->\n    <!--    </div>-->\n    <!--    <div class=\"two\">-->\n    <!--        <h3>Authorized by</h3>-->\n    <!--        <h3>____________________</h3>-->\n    <!--        <h4>Dr Mohamed Ibar</h4>-->\n    <!--        <h4>Laboratory Supervisor</h4>-->\n            \n    <!--    </div>-->\n    <!--    </div>-->\n     \n    <!--    <img class = \"mt-20\" src= \"/files/02-01.png\"/>-->\n\n    <!--</div>-->\n       \n\n          <!--<span class=\"px-3 mt-5 text-center\" style = \"background-color: #4f6e82 ; color: #fff\">Care Line : 0000  Tell : +254700254254   Email: info@zazihospitals.ke    web: zazihospitals.ke </span>-->\n        \n    <!--</div>-->\n    \n    <!--dhamaad footer cusub-->\n    \n {% endif %}\n\n \n ",
