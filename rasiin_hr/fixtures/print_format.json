  "html": "<!DOCTYPE html> \n<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Hodan hospital</title>\n    <!-- ..... css link -->\n    <link rel=\"stylesheet\" href=\"style.css\">\n    <!-- ......fontawseme -->\n    <link rel=\"stylesheet\" href=\"https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css\">\n</head>\n<body>\n    <section>\n        <!-- ........logo -->\n        <div class=\"main_logo\">\n            <div class=\"logo_left\">\n                <img src=\"/files/logo hodan.png\" alt=\"\">\n            </div>\n            <div class=\"right_logo\">\n                <h2>PATIENT MEDICAL</h2>\n                <h4>PRESCRIPTION</h4>\n            </div>\n        </div>\n        <!-- ........info -->\n        <div class=\"mian_info\">\n            <div class=\"info_1\">\n                <div class=\"info_id\">\n                    <h3>ID:</h3>\n                    <h4>{{doc.patient}}</h3>\n                </div>\n                <div class=\"info_age\">\n                    <h3>Age:</h3>\n                    <h4>{{frappe.db.get_value(\"Patient\",doc.patient,\"p_age\")}}</h3>\n                </div>\n                <div class=\"info_doctor\">\n                    <h3>Doctor:</h3>\n                    <h4>{{doc.practitioner_name}}</h3>\n                </div>\n                <div class=\"info_doctor\">\n                    <h3>GP Doctor:</h3>\n                    <h4>{{doc.gp_doctor}}</h3>\n                </div>\n            </div>\n            <div class=\"info_2\">\n                <div class=\"info_id_2\">\n                    <h3>Name:</h3>\n                    <h4>{{doc.patient_name}}</h3>\n                </div>\n                <div class=\"info_age_2\">\n                    <h3>sex:</h3>\n                    <h4 class=\"male\">{{doc.patient_sex}}</h3>\n                </div>\n                <div class=\"info_doctor_2\">\n                    <h3>Date:</h3>\n                    <h4 class=\"date\">{{doc.encounter_date}}</h3>\n                </div>\n            </div>\n        </div>\n    </section>\n\n    <!-- ........ table -->\n    <section class=\"table_section\">\n        <table>\n            <thead>\n                <tr>\n                    <th>No</th>\n                    <th>DRUG</th>\n                    <th>Dosage</th>\n                     <th>FREQUENCY</th>\n                    <th>QTY</th>\n                   \n                    <!--<th>ROUTE</th>-->\n                    <!--<th class=\"last\">INSTRACTION</th>-->\n                </tr>\n            </thead>\n\n            <tbody>\n                {% for drug in doc.drug_prescription %}\n                    <tr>\n                        <td>{{drug.idx}}</td>\n                        <td class=\"drug\">{{drug.drug_code}}</td>\n                        <td>{{drug.dosage}}</td>\n                      \n                        <td>{{drug.period}}</td>\n                          <td>{{drug.qty}}</td>\n                        <!--<td>{{drug.route}}</td>-->\n                        <!--<td class=\"last\">{{drug.instraction}}</td>-->\n                    </tr>\n                    <!--<tr>-->\n                    <!--    <td>1</td>-->\n                    <!--    <td>somaliya soomaali baa iska leh</td>-->\n                    <!--    <td>BOX</td>-->\n                    <!--    <td>3</td>-->\n                    <!--    <td>1</td>-->\n                    <!--    <td>somaliya soomaali baa iska leh</td>-->\n                    <!--    <td></td>-->\n                    <!--</tr>-->\n                {% endfor %}\n            </tbody>\n        </table>\n    </section>\n\n    <!-- ......footer -->\n    <section>\n        <div class=\"signature\">\n            <h3>Signature: ________________________</h3>\n        </div>\n        <div class=\"bukanka\">\n            <h3>Soo Laabashada Bunkaanka Waa  <span class=\"days\">{{frappe.db.get_value(\"Healthcare Settings\" , \"Healthcare Settings\" ,\"valid_days\")}} </span>Maalin Guahooda.</h3>\n        </div>\n        <div class=\"address\">\n            <h4>\n                <span><i class=\"fa fa-map-marker\"></i></span>\n                Hodan District, Banadir ,Abdikasim Road\n                <span><i class=\"fa fa-globe\"></i></span>\n                www.hodanhospital.so\n                <span><i class=\"fa fa-envelope\"></i></span>\n                info@hodanhospital.so\n                <span><i class=\"fa fa-phone\"></i></span>\n                0610000000\n            </h4>\n\n        </div>\n    </section>\n</body>\n</html>",
